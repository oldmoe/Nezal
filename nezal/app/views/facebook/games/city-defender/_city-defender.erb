<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
	<!--meta equiv="X-UA-Compatible" content="chrome=1"-->
	<head>
		<!--meta http-equiv="X-UA-Compatible" content="chrome=1"-->
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
		<link type="text/css" href="css/game.css" rel="stylesheet" />
		<script src="js/base/excanvas.compiled.js"></script>
		<script src="js/base/prototype.js"></script>
		<script src="js/base/effects.js"></script>
		<script src="js/base/sound.js"></script>
		<script src="js/base/template.js"></script>
		<script src="js/game/game.js"></script>
		<script src="js/game/images.js"></script>
		<script src="js/game/sounds.js"></script>
		<script src="js/game/unit.js"></script>
		<script src="js/game/bullets.js"></script>
		<script src="js/game/rockets.js"></script>
		<script src="js/game/ghost_turret.js"></script>
		<script src="js/game/creep.js"></script>
		<script src="js/game/tanks.js"></script>
		<script src="js/game/plane.js"></script>
		<script src="js/game/animation.js"></script>
		<script src="js/game/config.js"></script>
		<script src="js/game/mapeasy1.js"></script>
	</head>
	<body>
		<textarea id='towers_tpl' style="display:none">
		{for tower in towers}
			<div class="frame tower" title="${tower.name}" id="tower_${tower.name}" style="lefts: ${36*tower_index}px">
				<div class="border">
					<img src="images/cannon_base.png" />					
					<img class="top" src="images/cannon.png" />					
				</div>
			</div>
		{/for}
		</textarea>
		<textarea id='tpl' style="display:none">
			<h4>C3T</h4>
			
			<b>Air
			&<br/>Ground</b>
			<table>
				<tr>
					<th>Type</th>
					<td>Bullets</td>
				</tr>
				<tr>
					<th>Power</th>
					<td>100</td>
				</tr>
				<tr>
					<th>Range</th>
					<td>64</td>
				</tr>
				<tr>
					<th>Rate</th>
					<td>0.2</td>
				</tr>
				<tr>
					<th>HP</th>
					<td>230</td>
				</tr>
			</table>
			
		</table>
		</textarea>
		<div id="header">
		</div>
		<br/><br/>
		<br/><br/>
		<div id="gameContainer">
			<!--div id="canvasHeader">
				<span>Escaped :</span> <span id="escaped">0</span> 
				<span>Money :</span> <span id="money"></span>
			</div-->
			<div id="canvasContainer">
				<canvas id="gameBackground" width="736" height="480"></canvas>
				<canvas id="gameForeground" width="736" height="480"></canvas>
				
			</div>
			<div id="gameElements">
				<div class="status">
					<div id="score" class="score">0</div>
					<div id="lives" class="lives"></div>
				</div>
				<div class="fps"></div>
				<div class="rank"></div>
				<div class="start"></div>
				<div class="towers">
					<div class="tower1" title="Basic"></div>
					<div class="tower2" title="Double"></div>
					<div class="rocketLauncher" title="Rocket Launcher"></div>
					<div class="patriot" title="Patriot Air Defense"></div>
					<div></div>
					<div></div>
					<div></div>
					<div></div>
					<div></div>
					<div></div>
				</div>
				<div class="upgrades">
					<div class="upgradesBar"></div>
					<div class="upgradeItems">
							<div class="upgradeItem active"></div>
							<div class="upgradeItem active"></div>
							<div class="upgradeItem"></div>
							<div class="upgradeItem"></div>
							<div class="upgradeItem active"></div>
							<div class="upgradeItem active"></div>
							<div class="upgradeItem"></div>
							<div class="upgradeItem"></div>
					</div>
				</div>
				<div class="towerInfo">
					<div class="tower diagram"></div>
					<div id="towerData">
					<h4>C3T</h4>
					<b>Air
					&<br/>Ground</b>
					<table>
						<tr>
							<th>Type</th>
							<td>Bullets</td>
						</tr>
						<tr>
							<th>Power</th>
							<td>100</td>
						</tr>
						<tr>
							<th>Range</th>
							<td>64</td>
						</tr>
						<tr>
							<th>Rate</th>
							<td>0.2</td>
						</tr>
						<tr>
							<th>HP</th>
							<td>230</td>
						</tr>
					</table>
					</div>
				</div>
				<div class="controls">
					<div class="sound on"></div>
					<div class="like"></div>
					<!--div class="subscribe"></div-->
					<div class="bookmark"></div>
				</div>

				<div class="superWeapons">
					<div class="splash" onclick="Game.splash()">
					</div>
					<div class="heal" onclick="Game.heal()">
					</div>
					<div class="hyper" onclick="Game.hyper()">
					</div>
					<div class="weak" onclick="Game.slow()">
					</div>
					<div class="nuke" onclick="Game.nuke()">
					</div>
				</div>
				<div class="superWeaponsOff">
					<div class="splash">
					</div>
					<div class="heal">
					</div>
					<div class="hyper">
					</div>
					<div class="weak">
					</div>
					<div class="nuke">
					</div>
				</div>			</div>
			<canvas id="droppingGround" width="672" height="480"></canvas>
			<div id="result" style="display:none"></div>
	</body>
</html>
