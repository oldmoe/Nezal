    <style>
      .playerGrid{
        float : left;
      }
      .title {
        background : #333333;
        font-weight : bold;
        margin : 10px 8px 10px 0px;
        padding : 5px;
      }
      .grid {
        border :1px solid #333;
        background : #444;
        padding:0px;
        margin:0px;
        margin-right:8px;
        margin-bottom:8px;
        float: left;
      }
      .grid tr {
        color : black;
        background : white;
      }
      .grid td {
        border :1px solid #555  ;
        background : #151515;
        margin : 0px;
        padding : 0px;
        width : 28px;
        height : 30px;
      }
      .grid td.ship{
        background : orange;
      }
      .grid td.hit{
        background : #B01000;  
      }
      .grid td.miss{
        background : grey;
      }
      .grid td.wait{
        background : LightSkyBlue;  
      }
    </style>
    
    <script> 
 
      var player_num =  <%= @room[:users].length %> - 1//<%= ( user == @room[:users].keys[0]) ? 0 : ( user == @room[:users].keys[1 ])? 1: 2 %> 
      GameEngine.init();
      if(player_num < 2 )
      {
        GameEngine.join(player_num)
      }
    </script>

