<h1> Awesome Games List, Join one right now !!! </h1>
<% @games.each do |game | %>
  <div class="game">
    <div>
      <a href="/<%=AdminController::ADMIN_URL%>/<%=game.name%>"><%=game.name%></a>
      <span>
        <%=game.description%>
      </span>
    </div>
    <div style="clear:both"></div>
  </div>
<% end %>
